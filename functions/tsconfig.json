{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "ES2020",
    "skipLibCheck": true,
    "types": [
      "node"
    ],
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}